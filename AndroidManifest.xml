<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="syn.pos.mobile.iordertab"
    android:versionCode="1"
    android:versionName="1.7" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="16" />

    <application
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:logo="@drawable/logo"
        android:screenOrientation="portrait"
        android:theme="@android:style/Theme.Holo.Light">
        <activity
            android:name="syn.pos.mobile.iordertab.TakeOrderActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.AppConfigLayoutActivity"
            android:label="@string/activity_setting"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.MenuSetActivity"
            android:label="@string/activity_menu_set"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.MoveMenuActivity"
            android:label="@string/activity_move_menu"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.MoveMergeTable"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.CancelMenuActivity"
            android:label="@string/activity_cancel_menu"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.CheckBillActivity"
            android:label="@string/activity_checkbill"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.SearchMemberActivity"
            android:label="@string/activity_member_title"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.SetMemberFromMain"
            android:label="@string/activity_member_title"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.QueueActivity"
            android:label="@string/activity_queue"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden" >
        </activity>
        <activity android:name="syn.pos.mobile.iordertab.LogActivity" >
        </activity>
        <activity
            android:name="com.google.zxing.client.android.CaptureActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:windowSoftInputMode="stateAlwaysHidden" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.zxing.client.android.SCAN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.KdsInfoActivity"
            android:label="@string/activity_kds"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.ReprintMenuActivity"
            android:label="@string/reprint_quick_order"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="syn.pos.mobile.iordertab.RegisterActivity"
            android:label="@string/title_activity_register" android:windowSoftInputMode="stateAlwaysHidden">
        </activity>
    </application>

</manifest>